{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"],
      "@styles/*": ["UI/styles/*"],
      "@partials/*": ["UI/partials/*"],
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/UI/components/surfaces/Footer/Footer.styled.ts", "public/fonts/tw-icons/font/LocalFont.js"],
  "exclude": ["node_modules"]
}
